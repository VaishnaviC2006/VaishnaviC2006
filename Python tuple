# Tuple with duplicate values
thistuple = ("apple", "banana", "cherry", "apple", "cherry")
print(thistuple)

# Multiple types of tuples
tuple1 = ("apple", "banana", "cherry")
tuple2 = (1, 5, 7, 9, 3)
tuple3 = (True, False, False)
tuple4 = ("abc", 34, True, 40, "male")
print(type(tuple4))

# Using round brackets
tup = ("gfg", "Python")
print(tup)

# Without brackets (comma-separated values)
tup = 4, 5, 6
print(tup)

# Using tuple constructor
tup = tuple([7, 8, 9])
print(tup)

# Empty tuple
tup = ()
print(tup)

# Single-element tuple (with comma)
tup = (10,)
print(tup)
print(type(tup))

# Without comma - not a tuple
tup = (10)
print(tup)
print(type(tup))

# Using tuple() with another iterable
thistuple = tuple(("apple", "banana", "cherry"))
print(thistuple)

# Accessing tuple items
thistuple = ("apple", "banana", "cherry")
print(thistuple[1])
